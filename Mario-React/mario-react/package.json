    "colors": "1.4.0",
    "config": "3.1.0",
    "cross-env": "5.2.0",
    "customize-cra": "0.9.1",
    "dotenv": "8.0.0",
    "dotenv-webpack": "1.7.0",
    "es6-promise": "4.2.8",
    "es6-promise-promise": "1.0.0",
    "history": "4.9.0",
    "json-loader": "0.5.7",
    "jss": "9.8.7",
    "jss-preset-default": "4.5.0",
    "lodash": "^4.17.15",
    "nyc": "14.1.1",
    "prop-types": "^15.7.2",
    "qs": "6.9.1",
    "raf": "3.4.1",
    "react": "16.8.6",
    "react-app-polyfill": "1.0.1",
    "react-dev-utils": "9.0.1",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.8.8",
    "react-jss": "8.6.1",
    "react-redux": "7.1.0",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.4.0",
    "react-text-mask": "^5.4.3",
    "recharts": "^2.0.0-beta.1",
    "reduce-reducers": "1.0.4",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.3",
    "redux-persist": "5.10.0",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.3",
    "sinon": "7.5.0",
    "url-search-params-polyfill": "7.0.0",
    "uuid": "3.3.3",
    "webpack-split-chunks": "0.2.1"
  },
  "scripts": {
    "analyze:build": "cross-env ANALYZE=true WEBPACK_PATH=webpack.dev.config.js babel-node ./scripts/build.js",
    "build:analyze": "npm-run-all --parallel clean lint flow && npm run transpile && npm run analyze:build",
    "build": "npm run clean && npm run transpile && npm run build:app",
    "build:app": "cross-env NODE_ENV=production REACT_APP_ENV=production WEBPACK_PATH=webpack.prod.config.js babel-node ./scripts/build.js",
    "build:development": "npm run clean && npm run transpile && npm run build:appDevelopment",
    "build:appDevelopment": "cross-env NODE_ENV=production REACT_APP_ENV=production WEBPACK_PATH=webpack.dev.config.js babel-node ./scripts/build.js",
    "build:Integration": "npm run clean && npm run transpile && npm run build:appIntegration",
    "build:appIntegration": "cross-env NODE_ENV=integration REACT_APP_ENV=production WEBPACK_PATH=webpack.prod.config.js babel-node ./scripts/build.js",
    "build-storybook": "build-storybook",
    "clean": "rimraf dist",
    "flow": "flow",
    "flow:cov": "flow-coverage-report",
    "flow:restart": "flow stop && flow start",
    "lint": "cross-env NODE_ENV=local REACT_APP_ENV=local eslint ./src/** --fix",
    "serve:dev": "cross-env NODE_ENV=local REACT_APP_ENV=local webpack-dev-server --hot --color --config webpack.dev.config.js",
    "start:prod": "cross-env NODE_ENV=production REACT_APP_ENV=production webpack-dev-server --hot --color --config webpack.prod.config.js",
    "start:int": "cross-env NODE_ENV=integration REACT_APP_ENV=integration webpack-dev-server --hot --color --config webpack.prod.config.js",
    "start:development": "cross-env NODE_ENV=development REACT_APP_ENV=development webpack-dev-server --hot --color --config webpack.dev.config.js",
    "start:mac": "npm run serve:dev",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=local REACT_APP_ENV=local webpack-dev-server --hot --color --inline --progress --config webpack.dev.config.js",
    "test": "cross-env CI=true react-scripts test",
    "test:windows": "set CI=true && react-scripts test",
    "test:mac": "CI=true react-scripts test",
    "test:storybook": "concurrently 'npm run storybook' 'npm run storybook:wait && npm run test:all'",
    "transpile": "npm run clean && babel src -d dist --ignore \"src/**/*.test.js\",\"src/**/*.stories.js\"",
    "storybook": "start-storybook -p 6006"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootdir>/src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.5.5",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-actions": "5.2.6",
    "@storybook/addon-links": "5.2.6",
    "@storybook/addons": "5.2.6",
    "@storybook/react": "5.2.6",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "2.0.2",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "4.5.2",
    "core-js": "3.1.4",
    "css-hot-loader": "1.3.9",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.3.1",
    "enzyme-to-json": "3.3.4",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-react-app": "5.2.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-flowtype": "^3.1.4",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "file-loader": "2.0.0",
    "flow-babel-webpack-plugin": "1.1.1",
    "flow-bin": "0.99.1",
    "flow-coverage-report": "^0.6.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.4.3",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react-test-renderer": "16.4.1",
    "rimraf": "2.6.2",
    "url-loader": "1.1.2",
    "watch": "^1.0.2",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.4",
    "webpack-dev-server": "^3.1.10"
  }
}
